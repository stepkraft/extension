{
  "name": "react-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.6.0",
    "lodash": "^4.17.15",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-frame-component": "^4.1.1",
    "react-icons": "^3.8.0",
    "react-scripts": "3.2.0",
    "react-shadow": "^17.4.0",
    "recharts": "^2.0.0-beta.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "styled-components": "^5.0.0",
    "webextension-polyfill": "^0.5.0"
  },
  "scripts": {
    "build-semantic": "cd src/semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/semantic && npm run build-semantic && gulp watch",
    "start": "npm run build-semantic && react-app-rewired start",
    "build": "npm run build-semantic && INLINE_RUNTIME_CHUNK=false react-app-rewired build",
    "start__": "react-scripts start",
    "build__": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "start_": "react-app-rewired start",
    "build_": "INLINE_RUNTIME_CHUNK=false react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.5",
    "gulp": "^3.9.1",
    "react-app-rewired": "^2.1.5",
    "semantic-ui": "^2.4.2",
    "string-replace-webpack-plugin": "^0.1.3"
  }
}
